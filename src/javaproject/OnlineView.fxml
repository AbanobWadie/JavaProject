<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="550.0" prefWidth="750.0" style="-fx-background-color: #57D1C9;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="javaproject.OnlineViewController">
   <children>
      <ImageView blendMode="SCREEN" fitHeight="550.0" fitWidth="750.0" pickOnBounds="true">
         <image>
            <Image url="@../Images/wood.png" />
         </image>
      </ImageView>
      <Button fx:id="btn_play" layoutX="312.0" layoutY="353.0" mnemonicParsing="false" onAction="#play" prefHeight="37.0" prefWidth="126.0" text="Play" />
      <Label fx:id="lbl_player1" layoutX="150.0" layoutY="151.0" text="player1">
         <font>
            <Font name="System Bold Italic" size="22.0" />
         </font></Label>
      <Label fx:id="lbl_player2" layoutX="568.0" layoutY="151.0" text="Player2">
         <font>
            <Font name="System Bold Italic" size="22.0" />
         </font></Label>
      <RadioButton fx:id="X1" layoutX="173.0" layoutY="214.0" mnemonicParsing="false" onAction="#player1Symbol" text="X" />
      <RadioButton fx:id="O1" layoutX="173.0" layoutY="253.0" mnemonicParsing="false" onAction="#player1Symbol" text="O" />
      <RadioButton fx:id="X2" layoutX="587.0" layoutY="214.0" mnemonicParsing="false" onAction="#player2Symbol" text="X" />
      <RadioButton fx:id="O2" layoutX="587.0" layoutY="253.0" mnemonicParsing="false" onAction="#player2Symbol" text="O" />
      <Button fx:id="btn_record" layoutX="46.0" layoutY="492.0" mnemonicParsing="false" text="Record" />
      <Button fx:id="back" layoutX="650.0" layoutY="492.0" mnemonicParsing="false" onAction="#back" text="back" />
   </children>
</AnchorPane>
