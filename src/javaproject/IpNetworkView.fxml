<AnchorPane id="AnchorPane" prefHeight="550.0" prefWidth="750.0" style="-fx-background-color: #57D1C9;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="javaproject.IpNetworkViewController">
   <children>
      <ImageView blendMode="SCREEN" fitHeight="550.0" fitWidth="750.0" pickOnBounds="true">
         <image>
            <Image url="@../Images/xo.png" />
         </image>
      </ImageView>
      <TextField fx:id="txt_ip" layoutX="220.0" layoutY="178.0" prefHeight="39.0" prefWidth="311.0" promptText="Enter IP">
         <font>
            <Font size="14.0" />
         </font>
      </TextField>
      <Label layoutX="157.0" layoutY="183.0" text="IP:" textFill="#5e5c5c">
         <font>
            <Font name="System Bold" size="20.0" />
         </font>
      </Label>
      <Button fx:id="btn_play" layoutX="281.0" layoutY="303.0" mnemonicParsing="false" onAction="#play" prefHeight="44.0" prefWidth="189.0" style="-fx-background-color: #ED5485;" text="Connect" textFill="WHITE">
         <font>
            <Font name="System Bold Italic" size="20.0" />
         </font>
      </Button>
      <Hyperlink fx:id="btn_back" layoutX="13.0" layoutY="21.0" prefHeight="23.0" prefWidth="55.0" text="Back">
         <font>
            <Font name="Berlin Sans FB" size="14.0" />
         </font>
      </Hyperlink>
   </children>
</AnchorPane>
